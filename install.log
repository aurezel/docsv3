root .htaccess
RewriteRule ^eadropay/paydrb$ checkout/checkout.php [QSA,PT,L]
RewriteRule ^eadropay/notifydrb$ /checkout/pay/stckWebhook [QSA,PT,L]
RewriteRule ^eadropay/successdrb$ /checkout/pay/stckSuccess [QSA,PT,L]
RewriteRule ^eadropay/canceldrb$ /checkout/pay/stckCancel [QSA,PT,L]
RewriteRule ^eadropay/(.*)$ checkout/$1 [QSA,PT,L]


env
checkout_success_path = "/opalifypay/successs"
checkout_notify_path = "/opalifypay/notifys"
checkout_cancel_path = "/opalifypay/cancels"